{
  "name": "@swagger-api/apidom-json-path",
  "version": "1.0.0-beta.30",
  "description": "Evaluate JSONPath expressions against ApiDOM.",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "type": "module",
  "sideEffects": false,
  "unpkg": "./dist/apidom-json-path.browser.min.js",
  "main": "./dist/index.cjs",
  "exports": {
    "types": "./dist/apidom-json-path.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "types": "./dist/apidom-json-path.d.ts",
  "scripts": {
    "build": "tsup && api-extractor run -l -c ./config/api-extractor/api-extractor.json && npm run build:umd:browser",
    "build:umd:browser": "cross-env BABEL_ENV=browser webpack --config config/webpack/browser.config.js --progress",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swagger-api/apidom.git"
  },
  "author": "Vladim√≠r Gorej",
  "license": "Apache-2.0",
  "dependencies": {
    "@swagger-api/apidom-core": "^1.0.0-beta.30",
    "@swagger-api/apidom-error": "^1.0.0-beta.30",
    "@swagger-api/apidom-json-pointer": "^1.0.0-beta.30"
  },
  "devDependencies": {
    "@babel/runtime-corejs3": "^7.26.10",
    "jsonpath-plus": "^10.0.6",
    "tsup": "^8.4.0"
  },
  "files": [
    "src/**/*.mjs",
    "src/**/*.cjs",
    "dist/",
    "types/apidom-json-path.d.ts",
    "LICENSES",
    "NOTICE",
    "README.md",
    "CHANGELOG.md"
  ]
}
