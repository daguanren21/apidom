{
  "name": "@swagger-api/apidom-ls",
  "description": "ApiDOM based language service library.",
  "version": "1.0.0-beta.30",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
