{
  "name": "@swagger-api/apidom-ns-openapi-3-1",
  "version": "1.0.0-beta.30",
  "description": "OpenAPI 3.1.x namespace for ApiDOM.",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "type": "module",
  "sideEffects": [
    "./dist/refractor/registration.js",
    "./dist/refractor/registration.cjs"
  ],
  "unpkg": "./dist/apidom-ns-openapi-3-1.browser.min.js",
  "main": "./dist/index.cjs",
  "exports": {
    "types": "./dist/apidom-ns-openapi-3-1.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "./dist/apidom-ns-openapi-3-1.d.ts",
  "scripts": {
    "build": "tsup && api-extractor run -l -c ./config/api-extractor/api-extractor.json && npm run build:umd:browser",
    "build:umd:browser": "cross-env BABEL_ENV=browser webpack --config config/webpack/browser.config.js --progress",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swagger-api/apidom.git"
  },
  "author": "Vladimir Gorej",
  "license": "Apache-2.0",
  "dependencies": {
    "@swagger-api/apidom-ast": "^1.0.0-beta.30",
    "@swagger-api/apidom-core": "^1.0.0-beta.30",
    "@swagger-api/apidom-json-pointer": "^1.0.0-beta.30",
    "@swagger-api/apidom-ns-json-schema-2020-12": "^1.0.0-beta.30",
    "@swagger-api/apidom-ns-openapi-3-0": "^1.0.0-beta.30"
  },
  "devDependencies": {
    "@babel/runtime-corejs3": "^7.26.10",
    "@types/ramda": "~0.30.0",
    "ramda": "~0.30.0",
    "ramda-adjunct": "^5.0.0",
    "ts-mixer": "^6.0.3",
    "tsup": "^8.4.0"
  },
  "files": [
    "src/**/*.mjs",
    "src/**/*.cjs",
    "dist/",
    "types/apidom-ns-openapi-3-1.d.ts",
    "LICENSES",
    "NOTICE",
    "README.md",
    "CHANGELOG.md"
  ]
}
