{
  "name": "@swagger-api/apidom-error",
  "version": "1.0.0-beta.30",
  "description": "Backward compatible custom ApiDOM errors with causes.",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "type": "module",
  "sideEffects": false,
  "unpkg": "./dist/apidom-error.browser.min.js",
  "main": "./dist/index.cjs",
  "exports": {
    "types": "./dist/apidom-error.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "./dist/apidom-error.d.ts",
  "scripts": {
    "build": "tsup && api-extractor run -l -c ./config/api-extractor/api-extractor.json && npm run build:umd:browser",
    "build:umd:browser": "cross-env BABEL_ENV=browser webpack --config config/webpack/browser.config.js --progress",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swagger-api/apidom.git"
  },
  "author": "Vladim√≠r Gorej <vladimir.gorej@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/runtime-corejs3": "^7.20.7",
    "tsup": "^8.4.0"
  },
  "files": [
    "src/**/*.mjs",
    "src/**/*.cjs",
    "dist/",
    "types/apidom-error.d.ts",
    "LICENSES",
    "NOTICE",
    "README.md",
    "CHANGELOG.md"
  ]
}
